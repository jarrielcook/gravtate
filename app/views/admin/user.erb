Report for user: <%= @user.uid %>
<br/><br/><br/>

<table border="1">

<tr>
<th>Tag</th>
<th>Latitude</th>
<th>Longitude</th>
<th>Created</th>
</tr>

<% @user.references.each do |r| -%>

<tr>
<td><%= link_to r.tag.tag, :controller => :admin, :action => :tag, :id => r.tag.id %></td>
<td><%= link_to r.location.lat, :controller => :admin, :action => :location, :id => r.location.id %></td>
<td><%= link_to r.location.lon, :controller => :admin, :action => :location, :id => r.location.id %></td>
<td><%= r.created_at %></td>
</tr>

<% end -%>

</table>

<br/><br/><br/>
<%= link_to 'All Users', :controller => :admin, :action => :users %>
